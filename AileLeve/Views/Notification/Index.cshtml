@model AileLeve.ViewModels.NotificationViewModel

@{
    ViewBag.Title = "Liste des utilisateurs";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h1 class="text-center mb-4">Centre des notifications</h1>

<table class="table table-striped">
    <thead class="thead-light">
        <tr>
            <th scope="col">Description</th>
            <th scope="col">Statut</th>
    </tr>
    </thead>

<tbody>
   
    @foreach (var item in @Model.NotificationsListe)
    {

        <tr>
            <td>@item.TypeNotification</td>
            <td>@item.Lu</td>
        </tr>

    }
</tbody>
</table>